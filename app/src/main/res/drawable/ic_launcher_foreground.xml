<vector xmlns:android="http://schemas.android.com/apk/res/android" android:width="108dp" android:height="108dp" android:viewportWidth="108" android:viewportHeight="108">

    <!-- ZOOMED & CENTERED PIXEL ART -->
    <!-- Viewport 108x108. Safe Zone Center: 54,54. -->

    <!-- User Request: "Zoom out a bit" (Scale < 1.0) and "Center around money".
         The Money is in the lower body. So we must shift the block UP.
    -->

    <group android:pivotX="54" android:pivotY="54" android:scaleX="0.88" android:scaleY="0.88" android:translateY="-12">
        <!-- Scale 0.88 (Zoom out). Translate Y -12 (Shift Block Up to center Money). -->

        <!-- 1. DIRT BASE (Background) -->
        <path android:fillColor="#FF866043" android:pathData="M0,0 L108,0 L108,108 L0,108 Z" />

        <!-- 2. INNER FRAME -->
        <!-- 2. INNER FRAME REMOVED (Clean Dirt) -->

        <!-- 3. CENTER DIRT -->
        <path android:fillColor="#FF9E7B5C" android:pathData="M9,36 L99,36 L99,99 L9,99 Z" />

        <!-- 4. GRASS TOP (Faded) -->
        <path android:fillColor="#FF7CB342" android:pathData="M0,0 L108,0 L108,27 L0,27 Z" />
        <!-- Grass Shadow -->
        <path android:fillColor="#FF558B2F" android:pathData="M0,21 L108,21 L108,27 L0,27 Z" />

        <!-- 5. BANKNOTE (Target Center) -->
        <!-- Originally at Y=67 (approx). Shifting block up by 12 puts it at 55. Accurate. -->
        <group android:translateX="54" android:translateY="67">
            <group android:translateX="-27" android:translateY="-13" android:scaleX="1.5" android:scaleY="1.5">
                <!-- Banknote Base -->
                <path android:fillColor="#FFC5E1A5" android:pathData="M0,0 L36,0 L36,18 L0,18 Z" />

                <!-- Banknote Border -->
                <path android:fillColor="#FF558B2F" android:pathData="
                        M0,0 L36,0 L36,18 L0,18 Z
                        M3,3 L33,3 L33,15 L3,15 Z
                    " android:fillType="evenOdd" />

                <!-- Center Symbol: '5' -->
                <path android:fillColor="#FF33691E" android:pathData="
                        M16,5 L22,5 L22,7 L18,7 L18,8 L22,8 L22,12 L16,12 L16,10 L20,10 L20,9 L16,9 Z
                    " />

                <!-- Side Dots -->
                <path android:fillColor="#FF33691E" android:pathData="M6,6 L8,6 L8,8 L6,8 Z M6,10 L8,10 L8,12 L6,12 Z M28,6 L30,6 L30,8 L28,8 Z M28,10 L30,10 L30,12 L28,12 Z" />
            </group>
        </group>

    </group>

</vector>
